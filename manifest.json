{
    "name": "Clip Comments",
    "description": "",
    "version": "0.1.6",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage"
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["*://*.netflix.com/*"],
        "js": ["netflixPayload.js"]
    },
    {
        "matches": ["*://*.twitter.com/*"],
        "js": ["twitterPayload.js"]
    }],
    "icons": {
        "32": "logov1-32.png",
        "64": "logov1-64.png",
        "128": "logov1-128.png"
    }
  }
  